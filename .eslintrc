{
  parser: "babel-eslint",
  "plugins": [
  react
  ],
  "rules": {
    "comma-dangle": ["error", "never"],
    "max-len": [1, 120, 2, {ignoreComments: true}]
    },
  "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb-base"],
  "globals": {
      "React": true,
      "navigator": true,
      "location": true,
      "document": true,
      "describe": true,
      "it": true,
      "expect": true
    }
}
